<!DOCTYPE html>
<html lang="en">

    <head>

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}Vin C2C{% endblock %} </title>

        <!--@OCPlatformBundle/Resources/public/css/main.css-->
        {% stylesheets
  'assets/css/*'
'assets/bootstrap-select/dist/css/bootstrap-select.css'
'assets/bootstrap/css/bootstrap.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
        {% endstylesheets %}

        {% stylesheets 'assets/bootstrap-select/dist/css/bootstrap-select.css.map'
               output='css/bootstrap-select.css.map' %}
        {% endstylesheets %}

    {% block stylesheets %}{% endblock %}


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>



    <!--@OCPlatformBundle/Resources/public/js/main.js-->
    {% javascripts
        'assets/js/*'%}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <!-- Favicon and touch icons -->
    {#<link rel="shortcut icon" href="assets/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="assets/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="assets/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="assets/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="assets/ico/apple-touch-icon-57-precomposed.png">#}

</head>

<body>
    {% block body %}
    {% endblock %}

    {% block content %}
    {% block fos_user_content %}{% endblock %}
{% endblock %}

{% block javascripts %}
    <script type="text/javascript"
            src="{{ asset('assets/bootstrap/js/bootstrap.js') }}">
    </script>

    <script type="text/javascript"
            src="{{ asset('assets/bootstrap-select/dist/js/bootstrap-select.js') }}">
    </script>

    <script type="text/javascript"
            src="{{ asset('assets/bootstrap-select/dist/js/i18n/defaults-fr_FR.js') }}">
    </script>
{% endblock %}



</body>
</html>